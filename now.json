{
  "version": 2,
  "alias": "stakes.social",
  "public": true,
  "github": {
    "enabled": false
  },
  "builds": [
    {
      "src": "/packages/web/out/**",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/(0x.*)",
      "dest": "/packages/web/out/[propertyAddress].html",
      "headers": {
        "cache-control": "public, s-maxage=31536000"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/packages/web/out/$1",
      "headers": {
        "cache-control": "public, s-maxage=31536000"
      }
    }
  ]
}