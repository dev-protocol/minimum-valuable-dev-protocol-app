{
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/incubator/project/:project", "destination": "/incubator/project/[project]" },
    { "source": "/create/associate/:address", "destination": "/create/associate/[market]" },
    { "source": "/create/:address", "destination": "/create/[market]" },
    { "source": "/liquidity/v1", "destination": "/liquidity/[version]" },
    { "source": "/liquidity/v2", "destination": "/liquidity/[version]" },
    { "source": "/author/:address/edit", "destination": "/author/[authorAddress]/edit" },
    { "source": "/author/:address", "destination": "/author/[authorAddress]" },
    { "source": "/invite/:id", "destination": "/invite/[market]" },
    { "source": "/:address", "destination": "/[propertyAddress]" }
  ]
}
